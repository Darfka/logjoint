<format>
  <regular-grammar>
    <head-re><![CDATA[^(?<body>.*)$]]></head-re>
    <fields-config>
      <field name="Time"><![CDATA[SOURCE_TIME()]]></field>
      <field name="Body"><![CDATA[body]]></field>
    </fields-config>
    <patterns />
    <encoding>ACP</encoding>
  </regular-grammar>
  <id company="LogJoint" name="Generic Lines" />
  <description>Reads all the lines from any text file without any additional parsing.</description>
</format>