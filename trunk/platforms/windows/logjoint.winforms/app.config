<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.diagnostics>
    <sharedListeners>
      <add name="file" type="System.Diagnostics.TextWriterTraceListener" initializeData="debug.log" traceOutputOptions="DateTime, ProcessId, ThreadId">
      </add>
    </sharedListeners>
    <sources>
      <source name="TraceSourceApp" switchName="sourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="file" />
        </listeners>
      </source>
      <source name="UI" switchName="sourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="file" />
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="sourceSwitch" value="All"/>
    </switches>
    <trace autoflush="true" indentsize="2">
      <listeners>
        <clear/>
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>