<format>
  <regular-grammar>
    <head-re><![CDATA[(?<date>\d{4}\/\d{2}\/\d{2}\ \d{2}\:\d{2}\:\d{2})]]></head-re>
    <body-re><![CDATA[(?<body>.+)]]></body-re>
    <fields-config>
      <field name="Time"><![CDATA[TO_DATETIME(date, "yyyy/MM/dd HH:mm:ss")]]></field>
      <field name="Body"><![CDATA[body]]></field>
    </fields-config>
    <patterns>
      <pattern>*.log</pattern>
      <pattern>FSASWINS.LOG</pattern>
      <pattern>fsauains.LOG</pattern>
      <pattern>FSAVCSIN.LOG</pattern>
      <pattern>FSGUIINS.LOG</pattern>
      <pattern>FSPRODRM.LOG</pattern>
      <pattern>FSSGSUP.LOG</pattern>
      <pattern>fstnbins.LOG</pattern>
     </patterns>
		<encoding>ACP</encoding>
    </regular-grammar>
  <id company="F-Secure" name="instutil" />
  <description>Log files that are created witn DebugOutput function from instutil.cpp. Usually these logs are creared by setup plugins.</description>
</format>